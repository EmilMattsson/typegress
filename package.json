{
  "name": "typegress",
  "version": "1.0.0",
  "description": "express server in typescript and postgress db",
  "main": "server.ts",
  "scripts": {
    "start": "tsc && node dist/src/server.js",
    "start:watch": "nodemon"
  },
  "author": "Emil Mattsson",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "express": "^4.17.1",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/morgan": "^1.7.37",
    "nodemon": "^2.0.2"
  },
  "nodemonConfig": {
    "ignore": [
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "yarn start",
    "ext": "ts"
  }
}
